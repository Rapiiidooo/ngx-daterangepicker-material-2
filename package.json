{
  "name": "ngx-daterangepicker-material-2",
  "version": "5.0.8",
  "description": "Angular2+ date range picker (with material design theme)",
  "type": "module",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "ng2",
    "daterangepicker",
    "daterangepicker-2",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "homepage": "https://github.com/Rapiiidooo/ngx-daterangepicker-material-2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --configuration production --base-href=\"/ngx-daterangepicker-material-2/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p package.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rapiiidooo/ngx-daterangepicker-material-2.git"
  },
  "peerDependencies": {
    "dayjs": "^1.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular/animations": "^13.3.4",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "~12.2.17",
    "@angular/common": "^13.3.4",
    "@angular/compiler": "^13.3.4",
    "@angular/compiler-cli": "~13.3.4",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^13.3.4",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^13.3.4",
    "@angular/platform-browser-dynamic": "^13.3.4",
    "@angular/router": "^13.3.4",
    "@schematics/angular": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "^6.0.0",
    "core-js": "^2.5.4",
    "cpr": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "rimraf": "^2.6.1",
    "rxjs": "^6.3.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.3",
    "zone.js": "~0.11.4"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/daterangepicker/index.ts",
      "umdModuleIds": {
        "dayjs": "dayjs"
      }
    },
    "allowedNonPeerDependencies": [
      "dayjs"
    ],
    "dest": "dist"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json"
}
